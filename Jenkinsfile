pipeline {
  
  agent any
  stages {
    stages('Compile') {
      steps {
          echo 'compiling c program...'
      }
    }
    
    stages('Execute') {
      steps {
          echo 'texecuting c program...'
      }
    }
    
    stages('Deploy') {
      steps {
          echo 'deploying c program...'
      }
    }
    
  }
  
}
